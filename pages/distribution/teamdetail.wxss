.distributor-list{
  background-color: #fff;
}
.distributor-item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding:20rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.item-info{
  display: flex;
  flex-direction: row;
}
.item-info image{
  width: 60rpx;
  vertical-align:middle;
}
.item-info text{
  margin-left: 20rpx;
  font-size: 30rpx;
}
.item-cost{
  font-size: 26rpx;
}