.withdraw-box{
  margin-top: 10rpx;
  padding:20rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  background-color: #fff;
}
.withdraw-box picker{
  font-size: 30rpx;
}
.withdraw-box .picker text{
  color: blue;
}
.withdraw-input,
.card-input{
  margin: 20rpx 0;
  display: flex;
  flex-direction: row;
  font-size: 40rpx;
  border-bottom:1rpx solid #f5f5f5;
}
.card-input input{
  padding-left:10rpx; 
  width: 95%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 50rpx;
}
.card-input icon{
  width: 10%;
  margin-top: 20rpx;
}
.withdraw-input input{
  margin-left: 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 70rpx;
}
.withdrawBtn{
  width: 90%;
  margin:40rpx auto;
}
.message{
  top:60%;
  font-size: 30rpx;
}