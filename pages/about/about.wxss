.aboutTap{
  position: fixed;
  top:0;
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  z-index: 3
}
.aboutTap text{
  width: 33.33%;
  text-align: center;
}
.on{
  color: #00c3c9;
  border-bottom: 4rpx solid #00c3c9;
}

.container{
  margin-top: 80rpx;
}
.container image{
  width: 100%;
}
.about-map{
  margin-top: 80rpx;
}
.about-map map{
  width: 100%;
  height: 650rpx;
}
.map-btn{
  border-radius: 0;
}
.contact{
  margin-top: 20rpx;
}
.contact-item{
  position: relative;
  padding-left: 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  background-color: #fff;
  border-bottom: 1rpx solid #f5f5f5;
}
.contact-item image{
  width: 56rpx;
  vertical-align:middle;
}
.contact-item text{
  margin-left: 20rpx;
  font-size: 26rpx;
}